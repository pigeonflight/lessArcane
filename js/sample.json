{
"filename": "contentActionMenu-replace-with-bootstrap3-dropdown.xml",
"content": "<!-- transform your .contentActionMenus in your #edit-bar into following bootstrap dropdowns: -->\ <!-- due to Plone's css, which set's li tags to display:inline,\ the li class=\"divider\" will not show as a separator. The solution is to\ set your divider explicitly as display:block.\ -->\ <replace css:content=\"ul#contentActionMenus\">\ <ul class=\"nav nav-tabs\"><xsl:attribute name=\"id\"><xsl:value-of select=\".//dl/@id\"/></xsl:attribute>\ <xsl:for-each select=\"./li\">\ <li class=\"nav dropdown pull-right\">\ <a class=\"dropdown-toggle\" data-toggle=\"dropdown\" aria-expanded=\"true\">\ <xsl:for-each select=\".//dt/a//span\">\ <xsl:copy-of select='.' />\ </xsl:for-each>\ </a>\ \ <ul class=\"dropdown-menu\" role=\"menu\">\ <xsl:for-each select=\".//dd/ul/li\">\ <xsl:if test=\"@class='actionSeparator'\">\ <li role=\"presentation\" class=\"divider\"></li>\ </xsl:if>\ <li><a><xsl:attribute name=\"href\"><xsl:value-of select=\"./a/@href\" /></xsl:attribute> \ <xsl:copy-of select=\".//span\" /></a></li> \ </xsl:for-each>\ </ul>\ </li>\ </xsl:for-each>\ </ul>\ </replace>\ <drop attributes=\"id\" css:content=\"#edit-bar\" />\ <drop attributes=\"id\" css:content=\"#contentActionMenus\" />",
"unique_id": "ce67392460c3c08a",
"created_at": "2014-11-17T00:05:36Z",
"updated_at": "2014-11-17T02:20:51Z",
"from_gistbox": true
}
